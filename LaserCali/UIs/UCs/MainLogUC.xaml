<UserControl xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"  xmlns:dxschv="http://schemas.devexpress.com/winfx/2008/xaml/scheduling/visual"  x:Class="LaserCali.UIs.UCs.MainLogUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LaserCali.UIs.UCs"
             mc:Ignorable="d" MinWidth="300" MinHeight="200" Background="White">
    <Grid >
        <dxe:ListBoxEdit x:Name="listLog"
                        ItemsSource="{Binding ListPerson}" 
                         DisplayMember="Detail" ValueMember="Value"
                        FontFamily="Times New Roman" Margin="5,5,5,5" />
    </Grid>
</UserControl>
